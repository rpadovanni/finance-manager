{
  "name": "finance-manager",
  "author": {
    "name": "Rafael Padovani",
    "email": "rafael.pdvn@gmail.com"
  },
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "pnpm lint",
    "build": "pnpm db:generate && next build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettify": "prettier --write \"**/**/*.{js,jsx,ts,tsx,css,html,json}\"",
    "db:generate": "node_modules/.bin/prisma generate",
    "db:migrate": "node_modules/.bin/prisma migrate dev --name init",
    "db:seed": "pnpm dlx ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "db:studio": "node_modules/.bin/prisma studio",
    "test": "NODE_ENV=test jest --runInBand",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@next/env": "^15.1.0",
    "@prisma/client": "6.0.1",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.2",
    "@reduxjs/toolkit": "2.3.0",
    "@shadcn/ui": "0.0.4",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.460.0",
    "next": "15.0.3",
    "next-swagger-doc": "0.4.0",
    "next-themes": "^0.4.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.1",
    "react-number-format": "^5.4.2",
    "react-redux": "9.1.2",
    "recharts": "^2.15.0",
    "swagger-ui-react": "^5.18.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@faker-js/faker": "^9.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/swagger-ui-react": "^4.18.3",
    "dotenv": "^16.4.7",
    "eslint": "9.15.0",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "4.0.0-beta1",
    "msw": "^2.6.8",
    "postcss": "8.4.49",
    "prettier": "3.3.3",
